{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "scss.validate": false,
  "stylelint.validate": [
    "scss"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ffffff",
    "activityBar.background": "#ffffff",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#df9f9f",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#ffffff",
    "statusBar.background": "#e2f5df",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#bfe9b8",
    "statusBarItem.remoteBackground": "#e2f5df",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#e2f5df",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#e2f5df99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#e2f5df"
}
